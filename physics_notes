   \draw[-latex,color=black,thin] (0,-0.2) -- (0,1.4)node [anchor=east ,scale=1] {$a$};
    \draw (0.6,0.6) node [anchor=south west ,scale=1] {$a(t)$};
        \draw (0.5,0.25) node [anchor=center ,scale=1] {$\Delta v$};
        
 \end{tikzpicture}
 \hspace{1cm}
